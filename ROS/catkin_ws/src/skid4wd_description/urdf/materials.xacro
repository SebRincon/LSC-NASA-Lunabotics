<?xml version="1.0" ?>
<robot name="skid4wd" xmlns:xacro="http://www.ros.org/wiki/xacro" >


  <xacro:macro name="materials" params="name rgba">
    <material name="${name}">
      <color rgba="${rgba}"/>
    </material>  
  </xacro:macro>

<xacro:materials name="silver" rgba="0.700 0.700 0.700 1.000"/>
<xacro:materials name="orange" rgba="1.0 0.423529411765 0.0392156862745 1.0"/>

</robot>
